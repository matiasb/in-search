<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkListStore" id="results_store">
    <columns>
      <!-- column-name Torrent -->
      <column type="gchararray"/>
      <!-- column-name Seeds -->
      <column type="gint"/>
      <!-- column-name Peers -->
      <column type="gint"/>
      <!-- column-name Size -->
      <column type="gchararray"/>
      <!-- column-name Download -->
      <column type="gboolean"/>
      <!-- column-name Url -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkDialog" id="results_dialog">
    <property name="border_width">5</property>
    <property name="title" translatable="yes">Search Results</property>
    <property name="modal">True</property>
    <property name="window_position">center-on-parent</property>
    <property name="type_hint">normal</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="dialog-vbox1">
        <property name="visible">True</property>
        <property name="spacing">2</property>
        <child>
          <object class="GtkTreeView" id="results">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="model">results_store</property>
            <property name="enable_grid_lines">horizontal</property>
            <child>
              <object class="GtkTreeViewColumn" id="name">
                <property name="resizable">True</property>
                <property name="title">Torrent</property>
                <property name="expand">True</property>
                <child>
                  <object class="GtkCellRendererText" id="torrent_name"/>
                  <attributes>
                    <attribute name="markup">0</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="seeds">
                <property name="title">Seeds</property>
                <child>
                  <object class="GtkCellRendererText" id="seeds_text"/>
                  <attributes>
                    <attribute name="text">1</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="peers">
                <property name="title">Peers</property>
                <child>
                  <object class="GtkCellRendererText" id="peers_text"/>
                  <attributes>
                    <attribute name="text">2</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="Size">
                <property name="title">Size</property>
                <child>
                  <object class="GtkCellRendererText" id="size_text"/>
                  <attributes>
                    <attribute name="text">3</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="download">
                <child>
                  <object class="GtkCellRendererToggle" id="selected">
                    <signal name="toggled" handler="on_torrent_toggled"/>
                  </object>
                  <attributes>
                    <attribute name="active">4</attribute>
                  </attributes>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child internal-child="action_area">
          <object class="GtkHButtonBox" id="dialog-action_area1">
            <property name="visible">True</property>
            <property name="layout_style">end</property>
            <child>
              <object class="GtkButton" id="cancel_btn">
                <property name="label" translatable="yes">Close</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="download_btn">
                <property name="label" translatable="yes">Download Selected</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">False</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="2">cancel_btn</action-widget>
      <action-widget response="1">download_btn</action-widget>
    </action-widgets>
  </object>
</interface>
